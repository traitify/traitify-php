{
    "name": "traitify/client",
    "type": "library",
    "description": "Traitify API Client",
    "homepage": "https://www.traitify.com",
    "keywords": [
        "traitify",
        "api",
        "client",
        "personality"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Carson Wright",
            "email": "carsonnwright@gmail.com",
            "homepage": "https://www.traitify.com",
            "role": "Developer"
        },
        {
            "name": "Eric Fleming",
            "email": "efleming@gmail.com",
            "homepage": "https://www.traitify.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "guzzlehttp/guzzle": "4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*",
        "phpdocumentor/phpdocumentor": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Traitify\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        }
    },
    "support": {
        "issues": "https://github.com/traitify/client/issues",
        "source": "https://github.com/traitify/client"
    }
}
